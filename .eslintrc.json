{
  "env": {
    "node": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "import/extensions": ["error", { "js": "ignorePackages" }],
    "object-curly-newline": "off",
    "consistent-return": "off",
    "linebreak-style": 0,
    "import/prefer-default-export": "off"
  }
}
